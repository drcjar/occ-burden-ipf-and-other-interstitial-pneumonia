}

@Article{Cummings2013,
  author      = {Kristin J Cummings and Eva Suarthana and Nicole Edwards and Xiaoming Liang and Marcia L Stanton and Gregory A Day and Rena Saito and Kathleen Kreiss},
  title       = {Serial evaluations at an indium-tin oxide production facility.},
  journal     = {Am J Ind Med},
  year        = {2013},
  language    = {eng},
  volume      = {56},
  number      = {3},
  month       = {Mar},
  pages       = {300--307},
  doi         = {10.1002/ajim.22125},
  url         = {http://dx.doi.org/10.1002/ajim.22125},
  abstract    = {We evaluated the effectiveness of workplace changes to prevent indium lung disease, using 2002-2010 surveillance data collected by an indium-tin oxide production facility.We assessed pulmonary function using lower limits of normal. Blood indium concentration and personal air sampling data were used to estimate exposure.Abnormalities were uncommon at hire. After hire, prevalence of spirometric restriction was 31\% (n = 14/45), about fourfold higher than expected. Excessive decline in FEV1 was elevated at 29\% (n = 12/41). Half (n = 21/42) had blood indium ?5 µg/l. More recent hires had fewer abnormalities. There was a suggestion that abnormalities were more common among workers with blood indium ?5 µg/l, but otherwise an exposure-response relationship was not evident. Peak dust concentrations were obscured by time averaging.Evolving lung function abnormalities consistent with subclinical indium lung disease appeared common and merit systematic investigation. Traditional measures of exposure and response were not illustrative, suggesting fresh approaches will be needed. Workplace changes seemed to have had a positive though incomplete impact; novel preventive interventions are warranted.},
  file        = {Cummings2013.pdf:Cummings2013.pdf:PDF;Cummings2013.pdf:pdf/Cummings2013.pdf:PDF},
  institution = {Division of Respiratory Disease Studies, National Institute for Occupational Safety and Health, Centers for Disease Control and Prevention, Morgantown, WV 26505, USA. kcummings@cdc.gov},
  keywords    = {Adult; Air Pollutants, Occupational, adverse effects/analysis/blood; Biological Markers, blood; Forced Expiratory Volume; Humans; Indium, adverse effects/analysis/blood; Logistic Models; Lung Diseases, Interstitial, blood/chemically induced/diagnosis/prevention /&/ control; Male; Middle Aged; Occupational Diseases, blood/chemically induced/diagnosis/prevention /&/ control; Occupational Exposure, adverse effects/analysis/prevention /&/ control; Occupational Health; Population Surveillance; Program Evaluation; Spirometry; Time Factors; Tin Compounds, adverse effects},
  medline-pst = {ppublish},
  owner       = {sam},
