@Article{AbadFernandez2003,
  author      = {{Abad Fern{\'{a}}ndez}, A. and {de Miguel D{\'{\i}}ez}, J. and {L{\'{o}}pez Vime}, R. and {G{\'{o}}mez Santos}, D. and {N{\'{a}}jera Botello}, L. and {Jara Chinarro}, B.},
  title       = {[Lipoid pneumonia related to workplace exposure to paint].},
