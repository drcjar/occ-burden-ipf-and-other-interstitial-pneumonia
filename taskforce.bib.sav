@Article{Omae2011,
  author      = {Kazuyuki Omae and Makiko Nakano and Akiyo Tanaka and Miyuki Hirata and Tsutahiro Hamaguchi and Tatsuya Chonan},
  title       = {Indium lung--case reports and epidemiology.},
  journal     = {Int Arch Occup Environ Health},
  year        = {2011},
