@Article{Morell2013,
  Title                    = {Chronic hypersensitivity pneumonitis in patients diagnosed with idiopathic pulmonary fibrosis: a prospective case-cohort study.},
  Author                   = {Ferran Morell and Ana Villar and María-Ángeles Montero and Xavier Muñoz and Thomas V Colby and Sudhakar Pipvath and María-Jesús Cruz and Ganesh Raghu},
