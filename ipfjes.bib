@Article{Camidge2006,
  Title                    = {Factors affecting the mesothelioma detection rate within national and international epidemiological studies: insights from Scottish linked cancer registry-mortality data.},
  Author                   = {D. R. Camidge and D. L. Stockton and M. Bain},
  Journal                  = {Br. J. Cancer},
