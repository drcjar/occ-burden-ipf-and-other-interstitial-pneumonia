}

@Article{Kotaniemi2001,
  Title                    = {Does non-responder bias have a significant effect on the results in a postal questionnaire study?},
  Author                   = {J. T. Kotaniemi and J. Hassi and M. Kataja and E. Jönsson and L. A. Laitinen and A. R. Sovijärvi and B. Lundbäck},
